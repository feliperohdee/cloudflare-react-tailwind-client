{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "react-tailwind-client",
	"compatibility_date": "2025-10-11",
	"compatibility_flags": [
		"enable_nodejs_fs_module",
		"enable_nodejs_http_modules",
		"enable_nodejs_perf_hooks_module",
		"enable_nodejs_tty_module",
		"nodejs_compat"
	],
	"main": "worker/index.ts",
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "db",
			"database_id": "f5a94e93-c3c3-4c11-9b47-9f987d21e9cc",
			"migrations_dir": "./db/migrations",
			"remote": true
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "DO",
				"class_name": "Do"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["Do"]
		}
	],
	"rules": [
		{
			"fallthrough": true,
			"globs": ["**/*.sql"],
			"type": "Text"
		}
	],
	"vars": {
		"PRODUCTION": "true"
	}
}
