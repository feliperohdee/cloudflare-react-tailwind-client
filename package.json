{
	"name": "cloudflare-worker-starter",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"add-component": "npx shadcn@latest add",
		"build": "rm -rf dist && vite build",
		"cf-typegen": "wrangler types",
		"db:backup": "wrangler d1 export db --output=./db/backups/backup-$(date +%Y%m%d-%H%M%S).sql",
		"db:backup:local": "wrangler d1 export db --local --output=./db/backups/backup-$(date +%Y%m%d-%H%M%S).sql",
		"db:create": "wrangler d1 create db",
		"db:generate": "drizzle-kit generate",
		"db:info": "wrangler d1 info db",
		"db:list": "wrangler d1 list",
		"db:migrate": "wrangler d1 migrations apply db --remote",
		"db:migrate:list": "wrangler d1 migrations list db --remote",
		"db:migrate:list:local": "wrangler d1 migrations list db --local",
		"db:migrate:local": "wrangler d1 migrations apply db --local",
		"db:seed": "tsx db/seed.ts",
		"db:seed:local": "tsx db/seed.ts --local",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts",
		"db:studio:local": "drizzle-kit studio --config drizzle.config.local.ts",
		"deploy": "rm -rf dist && vite build && wrangler deploy",
		"dev": "vite dev",
		"lint": "prettier --write . && eslint .",
		"logs": "wrangler tail",
		"test": "vitest --run",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.17.1",
		"@cloudflare/vitest-pool-workers": "^0.10.15",
		"@eslint/js": "^9.30.1",
		"@libsql/client": "^0.15.15",
		"@nerdfolio/drizzle-d1-helpers": "^0.1.4",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/vite": "^4.1.11",
		"@types/bcryptjs": "^3.0.0",
		"@types/lodash": "^4.17.20",
		"@types/node": "^24.0.10",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"bcryptjs": "^3.0.3",
		"class-variance-authority": "^0.7.1",
		"cloudflare": "^5.2.0",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"drizzle-seed": "^0.3.1",
		"eslint": "^9.30.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"lodash": "^4.17.21",
		"lucide-react": "^0.525.0",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.19.2",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
		"use-good-hooks": "^1.0.30",
		"use-http-error": "^1.0.6",
		"use-infer": "^1.0.9",
		"use-json": "^1.0.1",
		"use-lite-react-router": "^1.0.32",
		"use-qs": "^1.0.5",
		"use-request-utils": "^1.0.81",
		"use-toastr": "^1.0.4",
		"vite": "^6.2.0",
		"vitest": "~3.2.0",
		"wrangler": "^4.54.0"
	}
}
